{{define "detail"}}
    {{template "header"}}
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <img src="/images/{{.Image}}" width="100%" class="img-fluid"/>
                <h2 class="mt-2">{{.Title}}</h2>
                <p class="mt-3">{{.Body}}</p>
                <p>{{parsedate .CreateAt}}</p>
            </div>
        </div>
    </div>
    {{template "footer"}}
{{end}}